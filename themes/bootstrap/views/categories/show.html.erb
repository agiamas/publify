<h2 class='page-header categoryTitle'><%= link_to @grouping.name, @grouping.permalink_url %></h2>
<div class='hfeed'>
  <%= render :partial => 'article', :collection => @articles %>  
</div>
<div id='paginate'>
  <%= paginate @articles, { :previous_label => _('Previous'), :next_label => _('Next')  } %>
</div>
